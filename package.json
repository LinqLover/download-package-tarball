{
  "name": "download-package-tarball",
  "version": "1.0.1",
  "description": "Download a node package as a tarball, for example from github or npm",
  "main": "dist/index.js",
  "scripts": {
    "test": "babel-tape-runner test.js && semistandard | snazzy",
    "build": "rm -rf dist && mkdir -p dist && babel lib --out-dir dist",
    "watch": "rm -rf dist && mkdir -p dist && babel -w lib --out-dir dist",
    "prepublish": "npm run build",
    "posttest": "readme package.json > readme.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kesla/download-package-tarball.git"
  },
  "author": "David Bj√∂rklund <david.bjorklund@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kesla/download-package-tarball/issues"
  },
  "homepage": "https://github.com/kesla/download-package-tarball#readme",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-tape-runner": "^2.0.1",
    "package-json-to-readme": "^1.5.1",
    "semistandard": "^8.0.0",
    "snazzy": "^4.0.0",
    "tapava": "^2.2.0",
    "tar-stream": "^1.5.2"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "co": "^4.6.0",
    "download-tarball": "^1.0.0",
    "mkdirp-then": "^1.2.0",
    "npm-package-arg": "^4.2.0",
    "rimraf-then": "^1.0.0",
    "then-fs": "^2.0.0",
    "then-read-json": "github:kesla/then-read-json",
    "tmp": "0.0.28"
  }
}
